import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a[]=new int[10];
        int n=sc.nextInt();
            int Sum=0;
          for(int i=0;i<n;i++)
        {
        a[i]=sc.nextInt();
             Sum=Sum+a[i];
        }
        System.out.print(Sum);
    }
}
    
